{
    "env": {
        "es6": true,
        "node": true
    },
    "globals": {
        "ENV": true,
        "AdWordsApp": true,
        "Charts": true,
        "DriveApp": true,
        "HTTPResponse": true,
        "Logger": true,
        "MailApp": true,
        "MccApp": true,
        "SpreadsheetApp": true,
        "UrlFetchApp": true,
        "Utilities": true,
        "XmlService": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            "error",
            2
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ]
    }
}